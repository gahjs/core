{
  "name": "@gah/cli",
  "version": "1.0.18",
  "homepage": "https://github.com/gahjs/core",
  "repository": {
    "type": "git",
    "url": "https://github.com/gahjs/core.git"
  },
  "description": "Generic Angular Host Tooling",
  "main": "./lib/public-api.js",
  "files": [
    "lib",
    "assets"
  ],
  "bin": {
    "gah": "./lib/index.js"
  },
  "scripts": {
    "prebuild": "del-cli lib",
    "postbuild": "cpy src/templates lib/templates && cpy ../README.md ./",
    "start": "yarn run build && yarn run global",
    "global": "yarn global add file:D:/git/@gahjs/core/cli",
    "lint": "yarn eslint src/**/*.ts",
    "build": "yarn run prebuild && tsc -p . && yarn run postbuild",
    "format": "prettier -w . --ignore-path .gitignore"
  },
  "engines": {
    "node": ">=12.0"
  },
  "author": "LoaderB0T",
  "license": "MIT",
  "private": false,
  "prettier": "@awdware/prettier-config",
  "dependencies": {
    "@gah/shared": "^1.0.10",
    "awesome-logging": "^0.5.1",
    "awilix": "^5.0.1",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "commander": "^9.1.0",
    "comment-json": "^4.2.2",
    "compare-versions": "^4.1.3",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "deep-equal": "^2.0.5",
    "ejs": "^3.1.6",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.0",
    "fswin": "^3.21.929",
    "globby": "^11.0.0",
    "simple-git": "^3.5.0"
  },
  "devDependencies": {
    "@awdware/eslint-config-base": "^1.0.0",
    "@awdware/eslint-config-nodejs": "^1.0.0",
    "@awdware/prettier-config": "^0.0.2",
    "@types/decompress": "^4.2.4",
    "@types/deep-equal": "^1.0.1",
    "@types/ejs": "^3.1.0",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^12.0.0",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "cpy-cli": "^3.1.1",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "legally": "^3.5.10",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
